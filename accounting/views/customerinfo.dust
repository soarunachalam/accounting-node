<HTML>
	<HEAD>
		<TITLE> User information</TITLE>
	</HEAD>

	<BODY>
		<H1>Balaji Agencies</H1>
		<script src="customerinfo_validate.js"> </script>
		
		<div class=ex1 style="width:350px;height:15px;">
			{#customerInfo}				
				{@eq key=state value=Added}
				   Customer information added
				{/eq}	
			{/customerInfo}	
		</div>
		</br>
		
		<form id='customerInfo' action='customer-information' onsubmit="return validateAndSubmit()" method='post'>
			{#customerInfo}
				<div class=ex style="width:750px;height:300px;position:relative;">
					<div style="position:absolute; left:0px; width:350px; height:350px;">
						<fieldset id="orgInfo" name="orgInfo">
							<h3>Organization details</h3>
							Name: &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="text" id="orgName" name="orgName" value="{name}"></br></br>
							Address Line1: &nbsp;&nbsp;<input type="text" id="addr1" name="addr1" value="{addr1}"></br></br>
							Address Line2: &nbsp;&nbsp;<input type="text" id="addr2" name="addr2" value="{addr2}"></br></br>
							Town: &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="text" id="town" name="town" value="{town}"></br></br>
							Area: &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<select id="area" name="area">
								<option value="select"> Select </option>
								<option value="area1"> Area1 </option>
								<option value="area2"> Area2 </option>
								<option value="area3"> Area3 </option>
							</select>
							</br></br>
							Phone: &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="text" id="orgPhone" name="orgPhone" value="{phone}"></br>
						</fieldset>
						</br>
					</div>
					<div style="position:absolute; left:380px; width:350px; height:350px;">
						<fieldset id="primeInfo" name="primeInfo">
							<h3>Primary contact</h3>
							Name: &nbsp;&nbsp;<input type="text" id="primeryContactName" name="primeryContactName" value="{prime_name}"></br></br>
							Phone: &nbsp;&nbsp;<input type="text" id="primeryContactPhone" name="primeryContactPhone" value="{prime_phone}"></br>
							</br></br></br></br></br></br></br></br>
						</fieldset>
					</div>
				</div>
				</br>
				<div style="position:relative; width:700px; left:300px">
					<input type="submit" value="submit"></input>
					&nbsp;&nbsp;&nbsp;
					<input type="reset" value="Reset"></input>
				</div>
			{/customerInfo}
		</form>
	</BODY>
</HTML>